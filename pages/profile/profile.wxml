<view class='container'>

  <view class='cell'>
    <text class='left'>大頭貼</text>
    <open-data class='avatar' type="userAvatarUrl" />
  </view>
  <view class='cell'>
    <text class='left'>名字</text>
    <open-data class='right' type="userNickName" />
  </view>
  <view class='cell' hidden='{{hiddenNickname}}'>
    <text class='left'>昵稱</text>
    <text class='right' >{{nickname}}</text>
  </view>
  <view class='cell'>
    <text class='left'>性別</text>
    <open-data class='right' type="userGender" lang='zh_TW' />
  </view>

  <view class='cell'>
    <text class='left'>地區</text>
    <open-data class='right' type="userProvince" lang='zh_TW' />
    <open-data id='city' class='right' type="userCity" lang='zh_TW' />
  </view>

  <view class='line bottom' />
</view>